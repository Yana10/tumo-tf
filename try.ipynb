    "total = 0\n",
    "\n",
    "def hanoi(n, frm, to, tmp):\n",
    "    global total\n",
    "    if n:\n",
    "        hanoi(n - 1, frm, tmp, to)\n",
    "        print (\"Move %d From %s To %s\" % (n, frm, to))\n",
    "        total += 1\n",
    "        hanoi(n - 1, tmp, to, frm)\n",
    "\n",
    "hanoi(4, 'A', 'C', 'B')\n",
    "print (\"total = \", total)"
